{
        "expo": {
                "name": "weekook",
                "slug": "weekook",
                "version": "1.0.0",
                "orientation": "portrait",
                "icon": "./assets/icon.png",
                "userInterfaceStyle": "automatic",
                "splash": {
                        "image": "./assets/splash.png",
                        "resizeMode": "cover"
                },
                "scheme": "com.weekook",
                "updates": {
                        "fallbackToCacheTimeout": 0
                },
                "assetBundlePatterns": [
                        "**/*"
                ],
                "ios": {
                        "supportsTablet": true,
                        "userInterfaceStyle": "automatic"
                },
                "android": {
                        "userInterfaceStyle": "automatic",
                        "adaptiveIcon": {
                                "foregroundImage": "./assets/adaptive-icon.png"
                        }
                },
                "web": {
                        "favicon": "./assets/favicon.png"
                }
        }
}